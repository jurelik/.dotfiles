:script ~/.vim/plugged/vim-tidal/Tidal.ghci

:{
  bpm b = setcps(b/60/4)
  e = every
  g = gain
  str = struct
  beginend b e = begin b # end e
  ripohm a b p = within (0.25, 0.75) (slow 2 . stutWith 8 (b/(-8)) (|* gain a)) p
  ripohm' a b c d p = within (c, d) (slow 2 . stutWith 8 (b/(-8)) (|* gain a)) p
  lfhf l h = lpf l # hpf h
:}
